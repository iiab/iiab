# Generated by IIAB
{% if iiab_lan_iface == "br0" %}
DEVICE=br0
TYPE=Bridge

{% else %}
DEVICE={{ iiab_lan_iface }}
TYPE=Ethernet
HWADDR={{ hostvars[inventory_hostname]['ansible_' + iiab_lan_iface]['macaddress'] }}

{% endif %}
BOOTPROTO=none
ONBOOT=yes
IPADDR={{ lan_ip }}
NETMASK={{ lan_netmask }}
NM_CONTROLLED=yes
DNS1=127.0.0.1
DOMAIN={{ iiab_domain }}
NAME=iiab-LAN
